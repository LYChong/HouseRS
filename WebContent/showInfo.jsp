							<td><%=house.getUsername()%></td>
							<td><%=house.getAddress()%></td>
							<td><%=house.getSalemoney()%></td>
							<td><%=house.getArea()%></td>
							<td><%=house.getHoutype()%></td>
							<td><%=house.getFloor()%></td>
							<td><%=house.getZhuangxiu()%></td>
							<td><%=house.getChaoxiang()%></td>
							<td><%=house.getLeasemoney()%></td>
							<td><a href="DeletHouseServlet?houno=<%=house.getHouno()%>">删除</a></td>
